plot_tree(tree)
# create the tree plot, flip it to horizontal and flip left-to-right
ggraph(tree, 'dendrogram') + coord_flip() + scale_y_reverse() +
# set the edge color scale to the terminal color to prevent transparency
scale_edge_colour_manual(values = color.terminal) +
# add the flipped edge arrows, hide the actual line
geom_edge_elbow(
aes(direction = 0, colour = color.terminal, y = y - 0.5),
start_cap = rectangle(10, 0.1999, 'native', 'native'),
arrow = edge_arrows,
edge_width = 0,
show.legend = FALSE
) +
# add angled parts of edges
geom_edge_fan(
aes(yend = y - 0.5)
) +
# add straight parts of edges, apply edge labels
geom_edge_fan(
aes(y = y - 0.5, x = xend, label = edge_labels),
label_pos = 0.33,
label_size = edge.label.size,
angle_calc = 'along'
) +
# set shape, size and color of nodes in tree
geom_node_point(
size = ifelse(layout$type == END, 0, vertex.size), # 0 if end vertex
shape = ifelse(layout$type == DECISION || layout$type == ROOT, 15, 16), # square/circle
color = ifelse(layout$type == ROOT, color.root, color.internal)
) +
# add node labels
geom_node_label(
aes(label = vertex_labels, vjust=),
# nudge_x = ifelse(layout$type == 1, 0.5, node.chance.nudge.x), # nudge intermediate node labels up
# nudge_y = ifelse(layout$type == 0, - node.label.nudge, ifelse(layout$type == 1, node.chance.nudge.y, node.label.nudge)), # nudge other labels out
# nudge_y = 1,
size = node.label.size
) +
# set axis titles, legends, etc.
theme(axis.text.y = element_blank(),
axis.text.x = element_blank(),
axis.ticks = element_blank(),
axis.title.x = element_blank(),
axis.title.y = element_blank(),
panel.background = element_blank(),
panel.grid.major = element_blank(),
panel.grid.minor = element_blank(),
plot.background = element_rect(fill = "white"),
panel.spacing = unit(2, "cm")
# panel.margin = margin(0, 50, 0, 0)
)
